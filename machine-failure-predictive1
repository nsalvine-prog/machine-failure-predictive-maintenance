   "source": [
    "import math\n",
    "#Membuat fitur baru, yaitu power, power wear, perbedaan temperatur, dan temperature power\n",
    "data['rotational_speed_rad'] = round(data['rotational_speed_rpm'] * (2 * 3.14159 / 60), 2)\n",
    "data['power'] = data['rotational_speed_rad'] * data['torque_nm']\n",
    "data['power wear']=data['power']*data['tool_wear_min']\n",
    "data['overstrain']=data['tool_wear_min']*data['torque_nm']\n",
    "data['temperature difference'] = data['process_temperature_k']-data['air_temperature_k']\n",
    "data['temperature power'] = data['temperature difference']/data['power']\n",
    "data['overstrain_typeL'] = data['overstrain']*data['type_L']\n",
    "data['overstrain_typeM'] = data['overstrain']*data['type_M']\n",
    "data['overstrain_typeH'] = data['overstrain']*data['type_H']\n",
    "data['rotational_speed_wear'] = data['rotational_speed_rpm'] * data['tool_wear_min']\n",
    "\n",
    "data = data[['air_temperature_k',\n",
    "         'process_temperature_k',\n",
    "         'rotational_speed_rpm',\n",
    "         'rotational_speed_rad',\n",
    "         'torque_nm',\n",
    "         'tool_wear_min',\n",
    "         'power',\n",
    "         'power wear',\n",
    "         'overstrain',\n",
    "         'temperature difference',\n",
    "         'temperature power', \n",
    "         'type_L','type_M','type_H','overstrain_typeL','overstrain_typeM','overstrain_typeH','rotational_speed_wear',\n",
    "         'machine_failure'\n",
    "        ]]"
   ]
  },

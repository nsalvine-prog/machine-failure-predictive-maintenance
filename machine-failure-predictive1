    "#Hitung nilai persentil ke-95 dan ke-99 dari kolom rotational_speed_rpm\n",
    "rpm_95 = data['rotational_speed_rpm'].quantile(0.95)\n",
    "rpm_99 = data['rotational_speed_rpm'].quantile(0.99)\n",
    "\n",
    "#Buat kolom baru '>95_rpm' dengan nilai 1 jika nilai 'rotational_speed_rpm' di atas persentil ke-95 dan ke-90, dan 0 jika tidak\n",
    "data['>95_rpm'] = (data['rotational_speed_rpm'] > rpm_95).astype(int)\n",
    "data['>99_rpm'] = (data['rotational_speed_rpm'] > rpm_99).astype(int)"
   ]

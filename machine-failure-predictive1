    "def clean_columns(data):\n",
    "  for column in data:\n",
    "    data = data.rename(\n",
    "        columns = {column : column.lower()\n",
    "        .replace(\" \", \"_\")\n",
    "        .replace(\"[\", \"\")\n",
    "        .replace(\"]\", \"\")})\n",
    "  return data"
   ]
  },

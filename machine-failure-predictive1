    "import matplotlib.pyplot as plt\n",
    "\n",
    "fig, axes = plt.subplots(3, 2, figsize=(10, 20), sharex=False)\n",
    "\n",
    "p9 = data['hdf'].value_counts().plot(kind='bar', figsize=(10, 15), ax=axes[0, 0])\n",
    "p10 = data['pwf'].value_counts().plot(kind='bar', figsize=(10, 15), ax=axes[0, 1])\n",
    "p11 = data['osf'].value_counts().plot(kind='bar', figsize=(10, 15), ax=axes[1, 0])\n",
    "p12 = data['rnf'].value_counts().plot(kind='bar', figsize=(10, 15), ax=axes[1, 1])\n",
    "p13 = data['twf'].value_counts().plot(kind='bar', figsize=(10, 15), ax=axes[2, 0])\n",
    "\n",
    "plots = [p9, p10, p11, p12, p13]\n",
    "\n",
    "for i in range(5):\n",
    "    plots[i].set(ylabel=None, xlabel=None)\n",
